{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/assets/webfonts/*": {
      "Access-Control-Allow-Origin": "*"
    }
  }
}
